{
    "name": "nw/json-request-bundle",
    "type": "symfony-bundle",
    "description": "Symfony JsonRequest Bundle",
    "keywords": ["json", "symfony", "bundle", "request", "api"],
    "homepage": "https://github.com/nowiko/JsonRequestBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NW\\JsonRequestBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "ext-json": "*",
        "php": "^7.3",
        "symfony/framework-bundle": "^4.3||^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpunit/php-code-coverage": "^5.0",
        "symfony/phpunit-bridge": "^4.3||^5.0",
        "symfony/yaml": "^4.3||^5.0"
    }
}
